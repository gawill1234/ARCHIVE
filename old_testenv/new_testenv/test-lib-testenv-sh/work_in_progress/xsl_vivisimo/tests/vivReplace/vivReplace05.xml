<testset>
   <test>
      <name>
         vivReplace05
      </name>
      <description>
         vivReplace():  regex substring replacement, different cases
      </description>
      <funcname argcnt="3" codetemplate="match" tbltemplate="1">
         viv:replace
      </funcname>
      <variable>
         <name>
            string1
         </name>
         <value>
            The Quick Brown fox jumps over the lazy dog
         </value>
         <type>
            string
         </type>
      </variable>
      <variable>
         <name>
            string2
         </name>
         <value>
            q[uv][hi].*n
         </value>
         <type>
            string
         </type>
      </variable>
      <variable>
         <name>
            string3
         </name>
         <value>
            slow
         </value>
         <type>
            string
         </type>
      </variable>
      <argument>
         <value>
            string1
         </value>
         <type>
            variable
         </type>
      </argument>
      <argument>
         <value>
            string2
         </value>
         <type>
            variable
         </type>
      </argument>
      <argument>
         <value>
            string3
         </value>
         <type>
            variable
         </type>
      </argument>
      <argument>
         <value>
            gi
         </value>
         <type>
            string
         </type>
      </argument>
      <result>
         <value>
            The slow fox jumps over the lazy dog
         </value>
         <type>
            string
         </type>
      </result>
   </test>
</testset>
