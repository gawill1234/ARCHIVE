<testset>
   <test>
      <name>
         vivMatch01
      </name>
      <description>
         vivMatch():  variable string matches identical variable string
      </description>
      <funcname argcnt="3" codetemplate="match" tbltemplate="1">
         viv:match
      </funcname>
      <variable>
         <name>
            string1
         </name>
         <value>
            abc
         </value>
         <type>
            string
         </type>
      </variable>
      <variable>
         <name>
            string2
         </name>
         <value>
            abc
         </value>
         <type>
            string
         </type>
      </variable>
      <argument>
         <value>
            string1
         </value>
         <type>
            variable
         </type>
      </argument>
      <argument>
         <value>
            string2
         </value>
         <type>
            variable
         </type>
      </argument>
      <result>
         <value>
            abc
         </value>
         <type>
            string
         </type>
      </result>
   </test>
</testset>
