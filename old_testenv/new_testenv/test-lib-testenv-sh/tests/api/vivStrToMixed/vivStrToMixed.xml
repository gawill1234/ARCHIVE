<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="/home/gaw/tryjunk/vivToUpper.xsl"?>
<testset>
   <test>
      <data>
      abcdefghijklmnopqrstuvwxyz
      </data>
      <expected>
      Abcdefghijklmnopqrstuvwxyz
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      ABCDEFGHIJKLMNOPQRSTUVWXYZ
      </data>
      <expected>
      Abcdefghijklmnopqrstuvwxyz
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      !@# $%^&amp;*()_+=-[]{};':",.&lt;>/?\|
      </data>
      <expected>
      !@# $%^&amp;*()_+=-[]{};':",.&lt;>/?\|
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      1234567890
      </data>
      <expected>
      1234567890
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      abcd1234
      </data>
      <expected>
      Abcd1234
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      AbCd 1234
      </data>
      <expected>
      Abcd 1234
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      aAaA;bBbB
      </data>
      <expected>
      Aaaa;bbbb
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      ;haliBUT-
      </data>
      <expected>
      ;halibut-
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      November 21, 1994
      </data>
      <expected>
      November 21, 1994
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      11/21/1994
      </data>
      <expected>
      11/21/1994
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      21-Nov-1994
      </data>
      <expected>
      21-Nov-1994
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      the quick brown fox jumps over the lazy dog
      </data>
      <expected>
      The Quick Brown Fox Jumps Over The Lazy Dog
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
   <test>
      <data>
      the quick brown fox jumps over the lazy dog
      </data>
      <expected>
      The quick brown fox jumps over the lazy dog
      </expected>
      <arguments>
      false()
      </arguments>
   </test>
   <test>
      <data>
      '    '
      </data>
      <expected>
      '    '
      </expected>
      <arguments>
      true()
      </arguments>
   </test>
</testset>
