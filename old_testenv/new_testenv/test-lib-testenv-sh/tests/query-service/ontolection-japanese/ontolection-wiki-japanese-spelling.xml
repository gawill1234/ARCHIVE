    <vse-index/>
    <converters>
      <converter timing-name="[word] pairs 2 OL" type-in="text/plain" type-out="application/vxml">
        <converter-test how="wc-set" what="url">*Jwiki-clean.txt</converter-test>
        <parser type="html-xsl">&lt;xsl:template match="text()"&gt;
  &lt;!--
  &lt;vse&gt;
    &lt;xsl:value-of select="." /&gt;
  &lt;/vse&gt;
--&gt;
  &lt;xsl:variable name="line" select="str:tokenize(., '&amp;#10;')" /&gt;
  &lt;vce&gt;
    &lt;xsl:apply-templates select="$line" mode="line" /&gt;
  &lt;/vce&gt;
&lt;/xsl:template&gt;

&lt;xsl:template match="*" mode="line"&gt;

  &lt;!-- processing plain text --&gt;
  &lt;xsl:variable name="pair" select="str:split(., '] [')" /&gt;

  &lt;!--
  &lt;xsl:variable name="line" select="." /&gt;
  &lt;xsl:variable name="pair" select="str:split($line, '] [')" /&gt; --&gt;
  &lt;xsl:variable name="first" select="substring-after($pair[1], '[')" /&gt;
  &lt;xsl:variable name="second" select="substring-before ($pair[2], ']')" /&gt;

  &lt;!-- creating docs with contents for first and second --&gt;
  &lt;document vse-key="{str:encode-uri($first, true())}" vse-key-normalized="vse-key-normalized"&gt;
    &lt;content name="spelling" action="none"&gt;
      &lt;xsl:value-of select="$first" /&gt;
    &lt;/content&gt;
    &lt;content name="spelling" action="none"&gt;
      &lt;xsl:value-of select="$second" /&gt;
    &lt;/content&gt;
  &lt;/document&gt;
&lt;/xsl:template&gt;
</parser>
      </converter>

      <converter type-in="vivisimo/fallback" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-unknown-to-vxml">
