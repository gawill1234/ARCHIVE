  <vse-config max-elt-id="2246">
    <crawler>
      <call-function name="vse-crawler-seed-files" type="crawl-seed">
<value-of-var name="install-dir" realm="option" />
/examples/data/ontolection-data/Jwiki-clean.txt</with>
