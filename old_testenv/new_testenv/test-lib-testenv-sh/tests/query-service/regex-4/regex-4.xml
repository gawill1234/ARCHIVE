<?xml version="1.0"?>
<vse-collection name="regex-4" elt-id="2185" max-elt-id="2185">
<vse-meta/>
  <vse-config>
    <crawler>
      <crawl-urls>
        <crawl-url url="http://vivisimo.com" enqueue-type="forced" status="complete">
          <curl-options>
            <curl-option name="default-allow" added="added">allow</curl-option>
            <curl-option name="max-hops" added="added">1</curl-option>
          </curl-options>
          <crawl-data content-type="application/vxml-unnormalized" encoding="xml">
            <vce>
              <document url="http://vivisimo.com?alpha">
                <content name="text">abcdefghijklmnopqrstuvwxyz</content>
                <content name="type">lower case alphabet</content>
              </document>
              <document url="http://vivisimo.com?alpha2">
                <content name="text">ABCDEFGHIJKLMNOPQRSTUVWXYZ</content>
                <content name="type">upper case alphabet</content>
              </document>
              <document url="http://vivisimo.com?odd1">
                <content name="text">! @ # $ % ^ &amp; * ( ) _ + = { } [ ] \ | ; : , . / ? &gt; &lt;</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?odd2">
                <content name="text">some chars ! @ # $ %</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?odd3">
                <content name="text">!@#$%^&amp;*()_+={}[]\|;:,./?&gt;&lt;</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?parens">
                <content name="text">container 1 ()</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?brackets">
                <content name="text">container 2 []</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?braces">
                <content name="text">container 3 {}</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?lines">
                <content name="text">for pictures - _ = ~</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?punctuation">
                <content name="text">Hello.  What, I ask, are you up to?  No; wait:  WOW!'"</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?math">
                <content name="text">123456789.0 +-=&lt;&gt;/*</content>
                <content name="type">odd characters</content>
              </document>
              <document url="http://vivisimo.com?numbers">
                <content name="text">1234567890</content>
                <content name="type">numbers</content>
              </document>
              <document url="http://vivisimo.com?atom">
                <content name="text">(atom)</content>
                <content name="type">atom look alike</content>
              </document>
              <document url="http://vivisimo.com?set">
                <content name="text">[set string]</content>
                <content name="type">set look alike</content>
              </document>
              <document url="http://vivisimo.com?atomset">
                <content name="text">9([^0-9a-z][A-Z])$</content>
                <content name="type">atom/set mix look alike</content>
              </document>
            </vce>
          </crawl-data>
        </crawl-url>
      </crawl-urls>
    </crawler>
    <vse-index>
      <vse-index-option name="term-expand-dicts">true</vse-index-option>
      <vse-index-option name="term-expand-dicts-dir">wc</vse-index-option>
      <vse-index-streams>
        <vse-index-stream stem="none" kb="none">
          <vse-tokenizer name="simple" word-chars="#()-@._!&amp;*^~=+[]{}\;:',&lt;&gt;/?" symbols=""/>
        </vse-index-stream>
        <vse-index-stream stem="case" kb="none">
          <vse-tokenizer name="simple" word-chars="#()-@._!&amp;*^~=+[]{}\;:',&lt;&gt;/?" symbols=""/>
        </vse-index-stream>
      </vse-index-streams>
    </vse-index>
    <converters><converter type-in="vivisimo/fallback" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-unknown-to-vxml">
          <with name="extract-strings">no, output XML</with>
        </call-function>
      </converter><converter type-in="unknown" type-out="dead">
        <call-function name="vse-converter-binary-files-in-archives"/>
      </converter><converter type-in="unknown" program="guess-content --type-override text/html text/html-to-utf8 %source_file" timing-name="Guess content"/><converter type-in="application/ms-office" program="ms-guess %source_file" timing-name="Guess MS Office"/><converter type-in="application/ms-office-container" program="unzip -l %source_file | ms-container-guess" timing-name="Guess MS Office 2007"/><converter type-in="application/word|application/excel|application/powerpoint" program="guess-office-2007 %source_file" timing-name="Check MS Office version"/><converter type-in="unknown" type-out="text/plain">
        <converter-test how="wc-set" what="path">*.txt</converter-test>
      </converter><converter type-in="unknown" type-out="application/excel">
        <converter-test how="wc-set" what="path">*.xls</converter-test>
      </converter><call-function name="vse-converter-filetypes"/><call-function name="vse-converter-type-normalization"/><converter type-in="application/vxml-db" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-database-tng"/>
      </converter><call-function name="vse-converter-powerpoint"/><call-function name="vse-converter-1-2-3"/><converter type-in="outlook/mail" type-out="text/mailbox">
        <call-function name="vse-converter-pst-to-mailbox"/>
      </converter><converter type-in="text/mailbox" type-out="text/mail">
        <call-function name="vse-converter-split-mailbox"/>
      </converter><converter type-in="text/mail" type-out="vivisimo/crawl-data">
        <call-function name="vse-converter-mail-message">
          <with name="cache-parser">vse-cache-parser-email</with>
        </call-function>
      </converter><converter type-in="application/universal" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-universal"/>
      </converter><converter type-in="application/documentum" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-documentum-to-xml"/>
      </converter><converter type-in="application/eroom" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-eroom-to-xml"/>
      </converter><converter type-out="application/vxml-unnormalized" type-in="application/lotus">
        <call-function name="vse-converter-lotus-to-xml"/>
      </converter><converter type-in="application/sharepoint" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-sharepoint-to-xml"/>
      </converter><converter type-in="application/io-sharepoint" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-io-sharepoint-to-xml"/>
      </converter><converter type-in="application/io-sharepoint-userprofile" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-sharepoint-user-profile-to-xml"/>
      </converter><converter type-in="application/word2" type-out="text/html">
        <call-function name="vse-converter-msword2"/>
      </converter><converter type-in="application/word" type-out="text/html">
        <call-function name="vse-converter-msword"/>
      </converter><converter type-in="application/excel" type-out="text/html">
        <call-function name="vse-converter-excel"/>
      </converter><converter type-in="application/wordperfect" type-out="text/html">
        <call-function name="vse-converter-wordperfect"/>
      </converter><converter type-in="application/openoffice" type-out="text/html">
        <call-function name="vse-converter-openoffice"/>
      </converter><converter type-in="application/ms-ooxml-word" type-out="text/html">
        <call-function name="vse-converter-ooxml-word"/>
      </converter><converter type-in="application/ms-ooxml-excel" type-out="text/html">
        <call-function name="vse-converter-ooxml-excel"/>
      </converter><converter type-in="application/ms-ooxml-powerpoint" type-out="text/html">
        <call-function name="vse-converter-ooxml-powerpoint"/>
      </converter><converter type-in="application/vnd.ms-project" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-ms-project"/>
      </converter><converter type-in="application/ps" type-out="text/plain">
        <call-function name="vse-converter-postscript"/>
      </converter><converter type-in="application/pdf" type-out="text/html">
        <call-function name="vse-converter-pdf"/>
      </converter><converter type-in="application/rtf" type-out="text/html">
        <call-function name="vse-converter-rtf"/>
      </converter><converter type-in="application/outlook-msg" type-out="vivisimo/crawl-data">
        <call-function name="vse-converter-outlook-msg"/>
      </converter><converter type-in="application/flash" type-out="text/html">
        <call-function name="vse-converter-flash"/>
      </converter><converter type-out="application/vxml-unnormalized" type-in="image/jpeg">
        <call-function name="vse-converter-jpg-metadata"/>
      </converter><converter timing-name="MP3 to metadata" type-in="audio/mpeg" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-mp3-metadata"/>
      </converter><converter type-in="application/tar" type-out="unknown">
        <call-function name="vse-converter-tar"/>
      </converter><converter type-in="application/x-gzip" type-out="unknown">
        <call-function name="vse-converter-gunzip"/>
      </converter><converter type-in="application/zip" type-out="unknown">
        <call-function name="vse-converter-unzip"/>
      </converter><converter type-in="application/x-compress" type-out="unknown">
        <call-function name="vse-converter-uncompress"/>
      </converter><converter type-in="text/html-to-utf8" type-out="text/html">
        <call-function name="vse-converter-html-to-utf8"/>
      </converter><converter type-in="text/html" type-out="application/vxml-unnormalized">
        <call-function name="vse-converter-html"/>
