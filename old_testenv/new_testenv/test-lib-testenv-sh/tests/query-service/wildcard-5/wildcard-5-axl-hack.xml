<application name="wildcard-5-axl-hack" modified-by="palmer@Vivisimo" max-elt-id="1" modified="1234455595">
  <call-function name="util-print-http-headers">
    <with name="type">text/plain</with>
  </call-function>
  <scope with-rights="admin-user">
  <print>
    <xml-to-text process="*">
        <call-function name="dictionary-build">
          <with name="dictionary">wildcard-5-dictionary</with>
        </call-function>
        <declare name="xml" type="nodeset" />
        <while test="not($xml) or count($xml/dictionary-status) != 1 or $xml/dictionary-status[not(@status = 'finished')]">
          <eval select="viv:sleep(1)" />
          <print>Still running....
</print>
          <set-var name="xml">
            <call-function name="dictionary-status-xml">
              <with name="dictionary">wildcard-5-dictionary</with>
            </call-function>
          </set-var>
        </while>
    </xml-to-text>
  </print>
  </scope>
</application>
