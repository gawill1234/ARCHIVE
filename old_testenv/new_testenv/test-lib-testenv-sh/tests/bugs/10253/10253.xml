<?xml version="1.0"?>
<vse-collection name="10253" modified="1208550235">
  <vse-config elt-id="2025" max-elt-id="2147">
    <crawler elt-id="2132">
      <call-function name="vse-crawler-seed-urls" elt-id="2125">
        <with name="urls" elt-id="2123">http://vivisimo.com</with>
        <with name="hops" elt-id="2124">1</with>
      </call-function>
      <crawl-options elt-id="2127">
        <crawl-option name="cache-types" elt-id="2126">
