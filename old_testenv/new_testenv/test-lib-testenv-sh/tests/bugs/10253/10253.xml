        <parser type="html-xsl" elt-id="2140">
&lt;xsl:template match="/"&gt; &lt;crawl-urls&gt; &lt;crawl-delete url="www.a.com" /&gt; &lt;crawl-delete url="www.b.com" /&gt; &lt;crawl-delete url="www.c.com" /&gt; &lt;crawl-url url="http://vivisimo.com/html/demos" /&gt; &lt;/crawl-urls&gt; &lt;/xsl:template&gt; 
</parser>
