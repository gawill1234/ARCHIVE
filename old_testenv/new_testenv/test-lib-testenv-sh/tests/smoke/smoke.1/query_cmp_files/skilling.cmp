  <content name="title" action="cluster">Moving forward: URGENT, URGENT.</content>
  <content name="cc-email" action="none">jeff.skilling@enron.com</content>
  <content name="cc-email" action="none">abelt@adventis.com</content>
  <content name="cc-email" action="none">mbruneau@adventis.com</content>
  <content name="cc-email" action="none">bkirby@adventis.com
</content>
  <content name="bcc-email" action="none">jeff.skilling@enron.com</content>
