  <content name="title" action="cluster">Talking Points - AT&amp;T</content>
  <content name="cc-email" action="none">david.cox@enron.com</content>
  <content name="cc-email" action="none">ken.rice@enron.com
</content>
  <content name="bcc-email" action="none">david.cox@enron.com</content>
