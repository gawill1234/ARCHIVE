#
#   Do not change
#   These must be set or nothing will work.
#
export TEST_ROOT=`pwd`
export PATH=$PATH:$TEST_ROOT/bin:$TEST_ROOT/utils/bin
export PYTHONPATH=$PYTHONPATH:$TEST_ROOT/lib

export ODBC_ROOT=$TEST_ROOT/odbc

export ORACLE_HOME=$ODBC_ROOT/oracle-10g
export ODBC_HOME=$ODBC_ROOT/unixODBC-2.2.12

export ODBCSYSINI=$ODBC_ROOT/etc
export ODBCINI=$ODBC_ROOT/odbc.ini

export LD_LIBRARY_PATH=$ORACLE_HOME/lib:$ODBC_HOME/lib

#######################################
#
#   Change to match your setup.
#   After, type ". setup"
#   RUN THIS IN THE DIRECTORY THIS FILE IS IN.
#
#   Your target host
#   Default is 127.0.0.1
#
#
#   This one is for the local install, if it exists
#   for doing things like testing transform or
#   local testing of the converters
#
#   As you can see, this is a vivisimo environment
#   variable.
#   gary_testing/mustang5
#   gary_testing5/mustang5
#   williams/n6GC8$?5
#   williams/n6GC8\$%3f5  (urlized pw)
#
export VIVISIMO_INSTALL_DIR=/usr/local/vivisimo-humana

#
#   Other local concerns like ANT_HOME or JAVA_HOME.
#
export ANT_HOME=/usr/java/ant
export JAVA_HOME=/usr/java/default
export CLASSPATH=.:$TEST_ROOT/lib:$TEST_ROOT/lib/velocity.jar:/usr/java/default/bin:/usr/java/default/lib:/usr/java/default/jre/lib:/usr/java/default/jre/ext:/usr/java/default/jre/lib/im:/usr/share/java:/usr/java/ant/lib:/usr/java/ant/lib/ant.jar:/usr/java/ant/lib/tools.jar:/usr/java/ant/lib/ant-launcher.jar

#########################################################
#
#   The rest is related to the target remote test
#   platform.
#
export OSSTRING="Suse Linux 10.1"
export VIVHOST=bielski-dev.bigdatalab.ibm.com
export VIVVERSION=7.5
export DEFAULTSUBCOLLECTION=live
export VIVTARGETARCH=linux64
#
#   Your target search port
#   Default is 7205
#
export VIVPORT=7205
#
#   Your target http port
#   Default is 80
#
export VIVHTTPPORT=80
#
#   Your target host operating system
#   Default is linux
#
export VIVTARGETOS=linux
#
#   Your Vivisimo user id
#   No default.  Must be set.
#
export VIVUSER=bielski
#export VIVUSER=test-upgrade
#
#   Your Vivisimo password
#   No default.  Must be set.
#
export VIVPW=t3stb3d
#   P@$$word#1?
#export VIVPW=P@\$\$word%231%3f
#
#   Your Vivisimo virtual directory
#   Default is vivisimo.
#
export VIVVIRTUALDIR=vivisimo
export VIVKILLALL=True

