#   These must be set or nothing will work.
#
export TEST_ROOT=`pwd`
export PATH=$PATH:$TEST_ROOT/bin:$TEST_ROOT/utils/bin
export PYTHONPATH=$PYTHONPATH:$TEST_ROOT/lib

