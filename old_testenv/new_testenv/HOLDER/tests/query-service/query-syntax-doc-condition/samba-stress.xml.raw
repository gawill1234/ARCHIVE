<source products="velocity" name="samba-stress" type="vse" test-strictly="test-strictly" modified="1225897754" modified-by="gary_testing">
  <prototype>
    <declare name="staging" initial-value="false" type="boolean">
      <description>Flag used to allow querying of the staging index.</description>
    </declare>
    <declare name="authorization-user" type="string">
      <label>Authorization user</label>
      <description>Username to use for document-level authentication of this source.</description>
    </declare>
    <declare name="authorization-password" type="password">
      <label>Authorization password</label>
      <description>Password to use for document-level authentication of this source.</description>
    </declare>
  </prototype>
  <submit>
    <form>
      <call-function name="vse_form">
        <with name="collection">samba-stress</with>
        <with name="authorization-user" copy-of="authorization-user"/>
        <with name="authorization-password" copy-of="authorization-password"/>
        <with name="staging" copy-of="staging"/>
      </call-function>
      <call-function name="vse-input-date"/>
    </form>
  </submit>
</source><source name="samba-stress" type="vse" test-strictly="test-strictly" modified="1224863866" modified-by="font@Vivisimo, Inc" max-elt-id="36">
