<testset>
   <test>
      <name>
         vivIfElse01
      </name>
      <description>
         vivIfElse():  Test of guaranteed true value using true() function.
      </description>
      <funcname argcnt="3" codetemplate="match" tbltemplate="1">
         viv:if-else
      </funcname>
      <variable>
         <name>
            myint
         </name>
         <value>
            100
         </value>
         <type>
            integer
         </type>
      </variable>
      <variable>
         <name>
            myfloat
         </name>
         <value>
            100.9
         </value>
         <type>
            float
         </type>
      </variable>
      <argument>
         <value>
            true()
         </value>
         <type>
            function
         </type>
      </argument>
      <argument>
         <value>
            TRUE
         </value>
         <type>
            string
         </type>
      </argument>
      <argument>
         <value>
            FALSE
         </value>
         <type>
            string
         </type>
      </argument>
      <result>
         <value>
            TRUE
         </value>
         <type>
            string
         </type>
      </result>
   </test>
</testset>
