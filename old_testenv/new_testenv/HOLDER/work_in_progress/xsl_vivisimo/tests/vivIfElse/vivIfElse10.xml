<testset>
   <test>
      <name>
         vivIfElse10
      </name>
      <description>
         vivIfElse():  Test of equal function condition
      </description>
      <funcname argcnt="3" codetemplate="match" tbltemplate="1">
         viv:if-else
      </funcname>
      <variable>
         <name>
            mybool
         </name>
         <value>
            true()
         </value>
         <type>
            boolean
         </type>
      </variable>
      <argument>
         <value>
            true() = $mybool
         </value>
         <type>
            vivtest
         </type>
      </argument>
      <argument>
         <value>
            TRUE
         </value>
         <type>
            string
         </type>
      </argument>
      <argument>
         <value>
            FALSE
         </value>
         <type>
            string
         </type>
      </argument>
      <result>
         <value>
            TRUE
         </value>
         <type>
            string
         </type>
      </result>
   </test>
</testset>
