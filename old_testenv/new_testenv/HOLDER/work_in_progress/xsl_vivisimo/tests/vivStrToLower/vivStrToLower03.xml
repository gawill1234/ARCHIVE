<testset>
   <test>
      <name>
         vivStrToLower03
      </name>
      <description>
         non-alphanum string variable remains unchanged
      </description>
      <funcname argcnt="3" codetemplate="match" tbltemplate="1">
         viv:str-to-lower
      </funcname>
      <variable>
         <name>
            string1
         </name>
         <value>
            !@# >&lt;*$%^&amp;()_+=-[]{};:,./?\|
         </value>
         <type>
            string
         </type>
      </variable>
      <argument>
         <value>
            string1
         </value>
         <type>
            variable
         </type>
      </argument>
      <result>
         <value>
            !@# >&lt;*$%^&amp;()_+=-[]{};:,./?\|
         </value>
         <type>
            string
         </type>
      </result>
   </test>
</testset>
